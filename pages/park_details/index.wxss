.details_main {
  width: 100%;
}
.details_main .details_top {
  background-color: #64b4ff;
  width: 100%;
  font-size: 32rpx;
  height: 80rpx;
  line-height: 32rpx;
  padding: 24rpx 40rpx;
  color: var(--themeColor);
}
.details_main .details_info_wrap {
  padding: 16rpx 40rpx;
}
.details_main .details_info_wrap .details_name_area_star {
  height: 60rpx;
  overflow: hidden;
}
.details_main .details_info_wrap .details_name_area_star .details_name {
  float: left;
  padding-right: 30rpx;
  font-size: 40rpx;
  font-weight: 600;
  line-height: 60rpx;
}
.details_main .details_info_wrap .details_name_area_star .details_area {
  background-color: #eee;
  color: #999;
  float: left;
  margin-top: 10rpx;
  font-size: 30rpx;
  height: 32rpx;
}
.details_main .details_info_wrap .details_name_area_star .details_star {
  float: right;
  width: 46rpx;
}
.details_main .details_info_wrap .details_name_area_star .details_star image {
  width: 100%;
}
.details_main .details_info_wrap .details_state {
  overflow: hidden;
}
.details_main .details_info_wrap .details_state .details_state_text {
  float: left;
  font-size: 20rpx;
  padding: 6rpx 6rpx;
  color: white;
  background-color: var(--themeColor);
  margin-right: 40rpx;
}
.details_main .details_info_wrap .details_address {
  height: 80rpx;
  font-size: 28rpx;
  line-height: 80rpx;
  color: grey;
}
.details_main .details_info_wrap .details_priceAndArea {
  display: flex;
  height: 160rpx;
  border-top: 8rpx solid #eee;
  border-bottom: 4rpx solid #eee;
}
.details_main .details_info_wrap .details_priceAndArea .details_price {
  flex: 1;
  height: 100%;
  padding: 20rpx 0;
}
.details_main .details_info_wrap .details_priceAndArea .details_price .details_price_title {
  text-align: center;
  font-size: 32rpx;
  font-weight: 500;
  height: 38rpx;
  border-right: 2rpx solid #ddd;
}
.details_main .details_info_wrap .details_priceAndArea .details_price .details_price_num {
  padding-top: 30rpx;
  text-align: center;
  font-size: 32rpx;
  font-weight: 500;
  height: 72rpx;
  border-right: 2rpx solid #ddd;
  font-weight: 600;
  color: #dc5d34;
}
.details_main .details_info_wrap .details_priceAndArea .details_area {
  flex: 1;
  height: 100%;
  padding: 20rpx 0;
}
.details_main .details_info_wrap .details_priceAndArea .details_area .details_area_title {
  text-align: center;
  font-size: 32rpx;
  font-weight: 500;
  height: 38rpx;
}
.details_main .details_info_wrap .details_priceAndArea .details_area .details_area_num {
  padding-top: 30rpx;
  text-align: center;
  font-size: 32rpx;
  font-weight: 500;
  height: 72rpx;
  font-weight: 600;
  color: #dc5d34;
}
.details_main .details_info_wrap .details_activity_wrap {
  border-top: 4rpx solid #eee;
  border-bottom: 4rpx solid #eee;
  padding: 40rpx 0;
}
.details_main .details_info_wrap .details_activity_wrap .details_activity_title {
  font-size: 40rpx;
  font-weight: 600;
  padding-bottom: 30rpx;
}
.details_main .details_info_wrap .details_activity_wrap .details_activity_range {
  padding-bottom: 10rpx;
  font-size: 32rpx;
}
.details_main .details_info_wrap .details_activity_wrap .details_activity_content {
  display: flex;
  flex-wrap: wrap;
}
.details_main .details_info_wrap .details_activity_wrap .details_activity_content .details_activity_content_item {
  width: 50%;
  padding-left: 30rpx;
  text-align: left;
  font-weight: 600;
  color: #dc5d34;
}
.details_main .details_info_wrap .details_item_info {
  border-top: 4rpx solid #eee;
  border-bottom: 4rpx solid #eee;
  padding: 40rpx 0;
}
.details_main .details_info_wrap .details_item_info .details_item_title {
  font-size: 40rpx;
  font-weight: 600;
  padding-bottom: 30rpx;
}
.details_main .details_info_wrap .details_item_info .details_park_num_wrap {
  display: flex;
  padding: 14rpx 0;
}
.details_main .details_info_wrap .details_item_info .details_park_num_wrap .details_park_num_title {
  font-size: 32rpx;
}
.details_main .details_info_wrap .details_item_info .details_park_num_wrap .details_park_num {
  font-size: 32rpx;
}
.details_main .details_info_wrap .details_item_info .details_park_category_wrap {
  display: flex;
  font-size: 32rpx;
  padding: 14rpx 0;
}
.details_main .details_info_wrap .details_item_info .details_park_type_wrap {
  display: flex;
  font-size: 32rpx;
  padding: 14rpx 0;
}
.details_main .details_info_wrap .details_item_info .details_park_developer_wrap {
  display: flex;
  font-size: 32rpx;
  padding: 14rpx 0;
}
.details_main .details_info_wrap .details_item_info .details_park_property_wrap {
  display: flex;
  font-size: 32rpx;
  padding: 14rpx 0;
}
.details_main .details_info_wrap .details_park_image_wrap {
  border-top: 4rpx solid #eee;
  border-bottom: 4rpx solid #eee;
  padding: 20rpx 0;
}
.details_main .details_info_wrap .details_park_image_wrap .details_park_image_title {
  font-size: 40rpx;
  font-weight: 600;
  padding: 14rpx 0;
}
.details_main .details_info_wrap .details_park_image_wrap .details_park_image {
  margin: 20rpx auto;
  width: 350rpx;
  height: 200rpx;
}
.details_main .details_info_wrap .details_park_image_wrap .details_park_image image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.details_main .details_info_wrap .details_park_payMoney {
  background-color: #eee;
  height: 130rpx;
  overflow: hidden;
}
.details_main .details_info_wrap .details_park_payMoney .details_park_payMoney_btn {
  margin: 20rpx auto;
  width: 560rpx;
  height: 88rpx;
  line-height: 88rpx;
  color: white;
  font-weight: 600;
  font-size: 40rpx;
  text-align: center;
  background-color: var(--themeColor);
}
