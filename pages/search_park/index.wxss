page {
  background-color: #dedede;
  padding-top: 20rpx;
  height: 100%;margin: 0;padding: 0;
}
.search_row {
  height: 60rpx;
  display: flex;
}
.search_row input {
  background-color: #fff;
  flex: 1;
  height: 100%;
  padding-left: 30rpx;
}
.search_row button {
  width: 110rpx;
  height: 100%;
  padding: 0;
  margin: 0 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
}
.search_content {
  margin-top: 30rpx;
}
.search_content .search_item {
  background-color: #fff;
  font-size: 26rpx;
  padding: 15rpx 10rpx;
  border-bottom: 1rpx solid #ccc;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}





map{
  height: 100vh;
  width: 100%;
}

.zan-dialog__mask {position: fixed;top: 0; left: 0;right: 0;bottom: 0;background: rgba(0, 0, 0, 0); background: rgba(0, 0, 0, 0.4);display: none;z-index: 1001}
  
.zan-dialog__container {position: fixed; bottom: 0;width: 100%;left: 0;right: 0;overflow: hidden;border-top-left-radius: 40rpx;
    box-shadow: darkgrey 0rpx 5rpx 15rpx 8rpx;z-index: 1003;
    background: #f8f8f8; margin: 0 auto;transform: translateY(300%);transition: all 0.4s ease;border-top-right-radius: 30rpx;}
  
.zan-dialog--show .zan-dialog__container {transform: translateY(0);}
.zan-dialog--show .zan-dialog__mask {display: block;}

.first_tab {
  width: 100%;
}
.first_tab .park_item {
  width: 100%;
  height: 280rpx;
  padding: 16rpx 36rpx;
  border-bottom: 2rpx solid grey;
  display: flex;
}
.first_tab .park_item .park_img_wrap {
  flex: 7;
  padding-right: 30rpx;
  margin-top: 18rpx;
}
.first_tab .park_item .park_img_wrap image {
  width: 280rpx;
  height: 200rpx;
}
.first_tab .park_item .park_info_wrap {
  flex: 9;
}
.first_tab .park_item .park_info_wrap .park_name_star {
  overflow: hidden;
  height: 60rpx;
}
.first_tab .park_item .park_info_wrap .park_name_star .park_name {
  float: left;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
}
.first_tab .park_item .park_info_wrap .park_name_star .park_star {
  float: right;
  width: 34rpx;
  line-height: 60rpx;
}
.first_tab .park_item .park_info_wrap .park_name_star .park_star image {
  width: 100%;
}
.first_tab .park_item .park_info_wrap .park_price {
  font-size: 28rpx;
  color: #dc5d34;
  padding-bottom: 10rpx;
}
.first_tab .park_item .park_info_wrap .park_region {
  font-size: 24rpx;
  color: grey;
  padding-bottom: 10rpx;
}
.first_tab .park_item .park_info_wrap .park_state {
  font-size: 20rpx;
  overflow: hidden;
}
.first_tab .park_item .park_info_wrap .park_state .park_state_text {
  float: left;
  margin-right: 20rpx;
  padding: 6rpx 6rpx;
  color: #fff;
  background-color: var(--themeColor);
}
.first_tab .park_item .park_info_wrap .park_time {
  font-size: 24rpx;
  padding-top: 10rpx;
}




