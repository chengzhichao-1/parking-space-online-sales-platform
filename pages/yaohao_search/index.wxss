Page {
  background-color: #eee;
}
.yaohao_search_main {
  width: 100%;
}
.yaohao_search_main .yaohao_search_search_wrap {
  margin: 20rpx auto;
  width: 670rpx;
  border-radius: 20rpx;
  box-shadow: 4rpx 4rpx 5rpx 5rpx #999;
}
.yaohao_search_main .yaohao_search_search_wrap .yaohao_search_search_title {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: var(--themeColor);
  color: #fff;
  text-align: center;
  font-weight: 550;
  font-size: 34rpx;
  border-radius: 20rpx 20rpx 0 0;
}
.yaohao_search_main .yaohao_search_search_info {
  width: 670rpx;
  padding: 60rpx;
  background-color: #fff;
  border-radius: 0 0 20rpx 20rpx;
}
.yaohao_search_main .yaohao_search_search_info .yaohao_search_search_info_threeInp input {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 20rpx;
  background-color: #eee;
  margin: 20rpx 0;
  text-align: center;
}
.yaohao_search_main .yaohao_search_search_info .yaohao_search_search_info_btn {
  margin: 60rpx auto;
  width: 230rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  color: #fff;
  background-color: var(--themeColor);
  border-radius: 60rpx;
  font-weight: 550;
  font-size: 30rpx;
}
.yaohao_search_main .yaohao_search_search_info .yaohao_search_search_info_item {
  font-size: 20rpx;
  text-align: center;
  color: #333;
}
.yaohao_search_main .yaohao_search_search_info .yaohao_search_search_info_item view {
  margin: 10rpx 0;
}
