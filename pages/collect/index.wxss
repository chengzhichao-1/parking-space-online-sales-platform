.collect_main .first_tab {
  width: 100%;
}
.collect_main .first_tab .park_item {
  width: 100%;
  height: 280rpx;
  padding: 16rpx 36rpx;
  border-bottom: 2rpx solid grey;
  display: flex;
}
.collect_main .first_tab .park_item .park_img_wrap {
  flex: 7;
  padding-right: 30rpx;
  margin-top: 18rpx;
}
.collect_main .first_tab .park_item .park_img_wrap image {
  width: 280rpx;
  height: 200rpx;
}
.collect_main .first_tab .park_item .park_info_wrap {
  flex: 9;
}
.collect_main .first_tab .park_item .park_info_wrap .park_name_star {
  overflow: hidden;
  height: 60rpx;
}
.collect_main .first_tab .park_item .park_info_wrap .park_name_star .park_name {
  float: left;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
}
.collect_main .first_tab .park_item .park_info_wrap .park_name_star .park_star {
  float: right;
  width: 34rpx;
  line-height: 60rpx;
}
.collect_main .first_tab .park_item .park_info_wrap .park_name_star .park_star image {
  width: 100%;
}
.collect_main .first_tab .park_item .park_info_wrap .park_price {
  font-size: 28rpx;
  color: #dc5d34;
  padding-bottom: 10rpx;
}
.collect_main .first_tab .park_item .park_info_wrap .park_region {
  font-size: 24rpx;
  color: grey;
  padding-bottom: 10rpx;
}
.collect_main .first_tab .park_item .park_info_wrap .park_state {
  font-size: 20rpx;
  overflow: hidden;
}
.collect_main .first_tab .park_item .park_info_wrap .park_state .park_state_text {
  float: left;
  margin-right: 20rpx;
  padding: 6rpx 6rpx;
  color: #fff;
  background-color: var(--themeColor);
}
.collect_main .first_tab .park_item .park_info_wrap .park_time {
  font-size: 24rpx;
  padding-top: 10rpx;
}
